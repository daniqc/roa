<h1>Listing resource_files</h1>

<table>
  <tr>
    <th>Resource</th>
    <th>Name</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @resource_files.each do |resource_file| %>
  <tr>
    <td><%= resource_file.resource_id %></td>
    <td><%= resource_file.name %></td>
    <td><%= link_to 'Show', resource_file %></td>
    <td><%= link_to 'Edit', edit_resource_file_path(resource_file) %></td>
    <td><%= link_to 'Destroy', resource_file, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Resource file', new_resource_file_path %>
