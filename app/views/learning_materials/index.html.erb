<h1>Listing learning_materials</h1>

<table>
  <tr>
    <th>Content</th>
    <th>Learning object</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @learning_materials.each do |learning_material| %>
  <tr>
    <td><%= learning_material.content_id %></td>
    <td><%= learning_material.learning_object_id %></td>
    <td><%= link_to 'Show', learning_material %></td>
    <td><%= link_to 'Edit', edit_learning_material_path(learning_material) %></td>
    <td><%= link_to 'Destroy', learning_material, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

